#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/robhogan/workspace/RN0730rc1/node_modules/.pnpm/@react-native-community+cli@12.0.0-alpha.17/node_modules/@react-native-community/cli/build/node_modules:/Users/robhogan/workspace/RN0730rc1/node_modules/.pnpm/@react-native-community+cli@12.0.0-alpha.17/node_modules/@react-native-community/cli/node_modules:/Users/robhogan/workspace/RN0730rc1/node_modules/.pnpm/@react-native-community+cli@12.0.0-alpha.17/node_modules/@react-native-community/node_modules:/Users/robhogan/workspace/RN0730rc1/node_modules/.pnpm/@react-native-community+cli@12.0.0-alpha.17/node_modules:/Users/robhogan/workspace/RN0730rc1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/robhogan/workspace/RN0730rc1/node_modules/.pnpm/@react-native-community+cli@12.0.0-alpha.17/node_modules/@react-native-community/cli/build/node_modules:/Users/robhogan/workspace/RN0730rc1/node_modules/.pnpm/@react-native-community+cli@12.0.0-alpha.17/node_modules/@react-native-community/cli/node_modules:/Users/robhogan/workspace/RN0730rc1/node_modules/.pnpm/@react-native-community+cli@12.0.0-alpha.17/node_modules/@react-native-community/node_modules:/Users/robhogan/workspace/RN0730rc1/node_modules/.pnpm/@react-native-community+cli@12.0.0-alpha.17/node_modules:/Users/robhogan/workspace/RN0730rc1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../build/bin.js" "$@"
else
  exec node  "$basedir/../../build/bin.js" "$@"
fi
